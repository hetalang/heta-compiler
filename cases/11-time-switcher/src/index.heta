
kabs @Const = 0.01; // constant of absorption
kel @Const = 0.01; // constant of elimination

a0 @Species {compartment: comp0, isAmount: true};
s1 @Species {compartment: comp1};
comp0 @Compartment;
comp1 @Compartment;
vabs @Reaction {actors: a0 -> s1};
vel @Reaction {actors: s1 ->};

comp0 .= 1;
comp1 .= 5.2;
vabs := kabs*a0;
vel := kel*s1*comp1;
a0 []= 0; // initial value
s1 []= 0;

