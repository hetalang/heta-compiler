sw1 @TimeSwitcher {
  start: 0, // start at zero time
  stop: 120,
  period: 12
};
a0 [sw1]= a0 + 10;
comp1 [sw1]= 6;

sw2_start @Const = 5;
sw2 @TimeSwitcher {
  start: sw2_start,
  period: 24
};
s1 [sw2]= s1 + 2;