k1 @Const = 0.01;

comp1 @Compartment .= 1;
x1 @Species { compartment: comp1 } .= 10;
x2 @Species { compartment: comp1 } .= 0;
r1 @Reaction { actors: x1->x2 } := k1*x1*comp1;

#export { filepath: sbml.xml, format: SBML };

// switcher
cond1 @Record := 6-x1;
sw1 @CSwitcher {
  condition: cond1
};
x1 [sw1]= 10;
