
default_comp @Compartment $mm;
default_comp $mm .= 1;

S @Species $mm { compartment: default_comp };
P @Species $mm { compartment: default_comp };
r1 @Reaction $mm { actors: S -> P };

S $mm .= 10;
P $mm .= 0;
r1 $mm := Vmax*S/(Km+S)*default_comp;

Vmax @Const = 0.1;
Km @Const = 2.5;

mm_sbml @SBMLExport { model: mm };
full_json @JSONExport;
full_yaml @YAMLExport;
mm_slv @SLVExport { model: mm };
mm_mrg @MrgsolveExport { model: mm };
