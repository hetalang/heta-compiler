
mm::default_comp @Compartment;
mm::default_comp .= 1;

mm::S @Species { compartment: default_comp };
mm::P @Species { compartment: default_comp };
mm::r1 @Reaction { actors: S -> P };

mm::S .= 10;
mm::P .= 0;
mm::r1 := Vmax*S/(Km+S)*default_comp;

Vmax @Const = 0.1;
Km @Const = 2.5;

mm::mm_sbml @SBMLExport;
mm::full_json @JSONExport;
mm::full_yaml @YAMLExport;
mm::mm_slv @SLVExport;
mm::mm_mrg @MrgsolveExport;
