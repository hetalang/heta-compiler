<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sbml 
  xmlns="http://www.sbml.org/sbml/level2/version4" 
  xmlns:xhtml="http://www.w3.org/1999/xhtml" 
  level="2" version="4"

  >
  <model 
    id="mm" 
    metaid="mm"
    >
    
    <listOfCompartments>
      <compartment
        id="default_comp"
        name="Default compartment"
        constant="false"
        size="1"
        units="_L"
        metaid="mm::default_comp"
        >
        <notes>
          <html xmlns="http://www.w3.org/1999/xhtml">
            <p>Note for the Default compartment</p>
          </html>
        </notes>
      </compartment>
    </listOfCompartments>
  
    <listOfSpecies>
      <species
        id="S"
        name="substrate"
        compartment="default_comp"
        constant="false"
        boundaryCondition="false"
        hasOnlySubstanceUnits="false"
        initialConcentration="10"
        substanceUnits="_uM_L"
        metaid="mm::S"
        >
      </species>
      <species
        id="P"
        name="product"
        compartment="default_comp"
        constant="false"
        boundaryCondition="false"
        hasOnlySubstanceUnits="false"
        initialConcentration="0"
        substanceUnits="_uM_L"
        metaid="mm::P"
        >
      </species>
    </listOfSpecies>
  
    <listOfParameters>
      <parameter
        id="Vmax"
        constant="true"
        value="0.1"
        units="_uM__minute"
        metaid="mm::Vmax"
        >
      </parameter>
      <parameter
        id="Km"
        constant="true"
        value="2.5"
        units="_uM"
        metaid="mm::Km"
        >
      </parameter>
    </listOfParameters>
  
    <listOfReactions>
      <reaction
        id="r1"
        name="Michaelis-Menten reaction"
        reversible="false"
        fast="false"
        metaid="mm::r1"
        >
        <notes>
          <html xmlns="http://www.w3.org/1999/xhtml">
            <p>Transformation of <strong>substrate</strong> to <strong>product</strong></p>
          </html>
        </notes>
        <listOfReactants>
          <speciesReference species="S" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P" stoichiometry="1"/>
        </listOfProducts>
        <listOfModifiers>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML"><apply><times/><apply><divide/><apply><times/><ci>Vmax</ci><ci>S</ci></apply><apply><plus/><ci>Km</ci><ci>S</ci></apply></apply><ci>default_comp</ci></apply></math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  
<!-- UNIT DEFINITIONS -->
<listOfUnitDefinitions>
  <unitDefinition id="_second" name="second">
  <listOfUnits>
    <unit kind="second" exponent="1" scale="0" multiplier="1"/>
  </listOfUnits>
</unitDefinition>
  <unitDefinition id="_L" name="L">
  <listOfUnits>
    <unit kind="litre" exponent="1" scale="0" multiplier="1"/>
  </listOfUnits>
</unitDefinition>
  <unitDefinition id="_uM_L" name="uM*L">
  <listOfUnits>
    <unit kind="mole" exponent="1" scale="-6" multiplier="1"/>
  </listOfUnits>
</unitDefinition>
  <unitDefinition id="_umole__minute" name="umole/minute">
  <listOfUnits>
    <unit kind="mole" exponent="1" scale="-6" multiplier="1"/>
    <unit kind="second" exponent="-1" scale="1" multiplier="6"/>
  </listOfUnits>
</unitDefinition>
  <unitDefinition id="_uM__minute" name="uM/minute">
  <listOfUnits>
    <unit kind="mole" exponent="1" scale="-6" multiplier="1"/>
    <unit kind="litre" exponent="-1" scale="0" multiplier="1"/>
    <unit kind="second" exponent="-1" scale="1" multiplier="6"/>
  </listOfUnits>
</unitDefinition>
  <unitDefinition id="_uM" name="uM">
  <listOfUnits>
    <unit kind="mole" exponent="1" scale="-6" multiplier="1"/>
    <unit kind="litre" exponent="-1" scale="0" multiplier="1"/>
  </listOfUnits>
</unitDefinition>
</listOfUnitDefinitions>
  
  </model>
</sbml>
