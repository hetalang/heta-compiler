{
  "name": "heta-compiler",
  "version": "0.5.3",
  "description": "Programming platform for Quantitative Systems Pharmacology modeling in NodeJS",
  "main": "src/index.js",
  "scripts": {
    "test:dev": "mocha --recursive --exclude **/*.tmp.js test/slv-export",
    "test": "mocha test --recursive --exclude **/*.tmp.js",
    "jsdoc": "jsdoc -r -c .jsdoc.json --readme api-references.md -d docs src",
    "test:cov": "nyc --reporter=lcov npm run test",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm run test | coveralls",
    "precompile": "nunjucks-precompile --include [\"\\.hjk$\"] src/templates > compiled-templates.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hetalang/heta-compiler.git"
  },
  "bugs": {
    "url": "https://github.com/hetalang/heta-compiler/issues",
    "email": "dev@insysbio.com"
  },
  "author": "Evgeny Metelkin <evgeny.metelkin@gmail.com> (https://github.com/metelkin)",
  "license": "Apache-2.0",
  "bin": {
    "heta": "bin/heta.js",
    "heta-build": "bin/heta-build.js",
    "heta-init": "bin/heta-init.js",
    "qs3p": "bin/heta.js",
    "qs3p-build": "bin/heta-build.js",
    "qs3p-init": "bin/heta-init.js"
  },
  "dependencies": {
    "@insysbio/topo-sort": "^1.0.1",
    "ajv": "^6.10.2",
    "ajv-errors": "^1.0.1",
    "colors": "^1.4.0",
    "commander": "^2.20.3",
    "fs-extra": "^7.0.1",
    "heta-parser": "^0.3.7",
    "inquirer": "^7.1.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "markdown": "^0.5.0",
    "mathjs": "^5.10.3",
    "mathjs-mathml": "^0.2.1",
    "mathjs-translate": "^0.1.8",
    "nunjucks": "^3.2.1",
    "random-id": "^1.0.3",
    "semver": "^6.3.0",
    "xlsx": "^0.15.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-xml": "^0.3.2",
    "coveralls": "^3.1.0",
    "jsdoc": "^3.6.3",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.1.1",
    "nyc": "^14.1.1",
    "slv-utils": "^0.3.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "private": false
}
