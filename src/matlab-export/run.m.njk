clear; clc; close all;

% global p;

p_names = [
{%- for con in constants -%}
"{{ con.id }}"{{ ', ' if not loop.last }}
{%- endfor -%}
];
y_names = [
{%- for record in dynamicRecords -%}
"{{ record.id }}"{{ ', ' if not loop.last }}
{%- endfor -%}
];

p = param();
y0 = init();

%%% default task
t_end = 1000;
[t, y] = ode15s(@(t, y) model(t, y, p), [0 t_end], y0);

figure
hold on
for i = 1:length(y_names)
    plot(t, y(:, i), '-', 'Linewidth', 2)
end
title('Default plot','Fontsize',14)
xlabel('t','Fontsize',14)
ylabel('records','Fontsize',14)
hold off
 