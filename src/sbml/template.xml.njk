<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" xmlns:xhtml="http://www.w3.org/1999/xhtml" level="2" version="4">
  <model>
  {% if out.listOfCompartments|length > 0 %}
    <listOfCompartments>
    {%- for record in out.listOfCompartments %}
      <compartment
        id="{{record.id}}"
        {% if record.title %}name="{{record.title}}"{% endif %}
        constant="false"
        {% if record.variable.size.num!==undefined and record.kind!=='rule' -%}size="{{record.variable.size.num}}"{%- endif %}
        {% if record.variable.units!==undefined -%}units="{{ record.unitsHash }}"{%- endif %}
        metaid="{{record.index}}"
        >
        {% if record.notes %}<notes><html xmlns="http://www.w3.org/1999/xhtml">{{record.notesHTML}}</html></notes>{% endif %}
      </compartment>
    {%- endfor %}
    </listOfCompartments>
  {% endif %}
  {% if out.listOfSpecies|length > 0 %}
    <listOfSpecies>
    {%- for record in out.listOfSpecies %}
      <species
        id="{{record.id}}"
        {% if record.title %}name="{{record.title}}"{% endif %}
        hasOnlySubstanceUnits="{{record.isAmount===true}}"
        speciesType=""
        compartment="{{record.compartment}}"
        constant="false"
        boundaryCondition="{{ record.variable.kind!=='dynamic' }}"
        {% if record.variable.size.num!==undefined and record.variable.kind!=='rule' -%}initialConcentration="{{record.variable.size.num}}"{%- endif %}
        {% if record.variable.units!==undefined -%}substanceUnits="{{record.unitsHash}}"{%- endif %}
        metaid="{{record.index}}"
        >
        {% if record.notes %}<notes><html xmlns="http://www.w3.org/1999/xhtml">{{record.notesHTML}}</html></notes>{% endif %}
      </species>
    {%- endfor %}
    </listOfSpecies>
  {% endif %}
  {% if out.listOfParameters|length > 0 %}
    <listOfParameters>
    {%- for record in out.listOfParameters %}
      <parameter
        id="{{record.id}}"
        {% if record.title %}name="{{record.title}}"{% endif %}
        constant="false"
        {% if record.variable.size.num!==undefined and record.variable.kind!=='rule' -%}value="{{record.variable.size.num}}"{%- endif %}
        {% if record.variable.units!==undefined -%}units="{{record.unitsHash}}"{%- endif %}
        metaid="{{record.index}}"
        >
        {% if record.notes %}<notes><html xmlns="http://www.w3.org/1999/xhtml">{{record.notesHTML}}</html></notes>{% endif %}
      </parameter>
    {%- endfor %}
  </listOfParameters>
  {% endif %}
  {% if out.listOfReactions|length > 0 %}
    <listOfReactions>
    {%- for record in out.listOfReactions %}
      <reaction
        id="{{record.id}}"
        {% if record.title %}name="{{record.title}}"{% endif %}
        reversible="{{ record.aux.reversible!=='false' }}"
        fast="{{ record.aux.fast==='true' }}"
        metaid="{{record.index}}"
        >
        {% if record.notes %}<notes><html xmlns="http://www.w3.org/1999/xhtml">{{record.notesHTML}}</html></notes>{% endif %}
        <listOfReactants>
          {% for actor in record.actors -%}
          {% if actor.stoichiometry<0 %}<speciesReference species="{{ actor.target }}" stoichiometry="{{ -actor.stoichiometry }}"/>{% endif %}
          {% endfor -%}
        </listOfReactants>
        <listOfProducts>
          {% for actor in record.actors -%}
          {% if actor.stoichiometry>0 %}<speciesReference species="{{ actor.target }}" stoichiometry="{{ actor.stoichiometry }}"/>{% endif %}
          {% endfor -%}
        </listOfProducts>
        <listOfModifiers>
          {% for effector in record.effectors -%}
          <modifierSpeciesReference species="{{ effector.target }}"/>
          {% endfor -%}
        </listOfModifiers>
        <kineticLaw>
          {{record.variable.size.toCMathML}}
        </kineticLaw>
      </reaction>
    {%- endfor %}
  </listOfReactions>
  {% endif %}
  <listOfRules>
  {%- for record in out.listOfRules %}
    <assignmentRule
      variable="{{record.id}}"
      {% if record.title %}name="{{record.title}}"{% endif %}
      >
      {{record.variable.size.toCMathML}}
    </assignmentRule>
  {%- endfor %}
  {%- for record in out.listOfProcesses %}
    {%- for actor in record.actors %}
    <rateRule
      variable="{{actor.target}}"
      {% if record.title %}name="{{record.title}}"{% endif %}
      >
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <times/>
          <cn>{{actor.stoichiometry}}</cn>
          <ci>{{record.id}}</ci>
        </apply>
      </math>
    </rateRule>
    {%- endfor %}
  {%- endfor %}
  </listOfRules>
  {% if out.listOfInitialAssignments|length > 0 %}
    <listOfInitialAssignments>
    {%- for record in out.listOfInitialAssignments %}
      <initialAssignment
        symbol="{{record.id}}"
        {% if record.title %}name="{{record.title}}"{% endif %}
        >
        {{record.variable.size.toCMathML}}
      </initialAssignment>
    {%- endfor %}
    </listOfInitialAssignments>
  {% endif %}
  {% if out.listOfEvents %}
    <listOfEvents>
      {%- for event in out.listOfEvents %}
      <event
        >
        <trigger>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <geq/>
              <ci>{{ event.id }}</ci>
              <cn>0</cn>
            </apply>
          </math>
        </trigger>
        <listOfEventAssignments>
          {% for assignment in event.assignments %}
          <eventAssignment
            variable="{{ assignment.target }}">
            {{ assignment.size.toCMathML }}
          </eventAssignment>
          {% endfor %}
        </listOfEventAssignments>
      </event>
      {%- endfor %}
    </listOfEvents>
  {% endif %}
  {% if out.uniqueUnits|length > 0 %}
    <listOfUnitDefinitions>
    {%- for record in out.uniqueUnits %}
      {{ record.getUnitDefinition() }}
    {%- endfor %}
  </listOfUnitDefinitions>
  {% endif %}
  </model>
</sbml>
